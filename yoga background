let bg;
let y = 0;
function setup() {
  
  bg = loadImage('https://burst.shopifycdn.com/photos/woman-practices-yoga-under-a-large-tree.jpg?width=1850&format=pjpg&exif=1&iptc=1/woman-practices-yoga-under-a-large-tree_ZImatwT0.jpg') ;

 createCanvas(720, 400);
}
function draw() {
  
 background(bg);

  stroke(226, 204, 0);
  line(0, y, width, y);

  y++;
  if (y > height) {
    y = 0;
  }
}
